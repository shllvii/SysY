void hanoi(int n, int a, int b, int c) {
  if (n == 1) {
    putint(a);
    putint(c);
    int newline = 10;
    putch(newline);
  } else {
    hanoi(n-1, a, c, b);
    putint(a);
    putint(c);
    int newline = 10;
    putch(newline);
    hanoi(n-1, b, a, c);
  }
}

int main()
{ 
  int n;
	n=getint();
  hanoi(n, 1, 2, 3);
  int newline = 10;
  putch(newline);
	return 0;
}